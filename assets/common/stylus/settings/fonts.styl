//Gibson font
@font-face
  font-family 'GibsonLight'
  src url('../fonts/gibson-light.woff') format('woff'), url('../fonts/gibson-light.ttf') format('truetype')

@font-face
  font-family 'GibsonRegular'
  src url('../fonts/gibson-regular.woff') format('woff'), url('../fonts/gibson-regular.ttf') format('truetype')

@font-face
  font-family 'GibsonSemibold'
  src url('../fonts/gibson-semibold.woff') format('woff'), url('../fonts/gibson-semibold.ttf') format('truetype')


