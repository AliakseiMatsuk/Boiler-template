mixin s-intro(o = {})

  +b.SECTION.s-intro
    +e.image(src=`./images/photos/${o.overlay}.jpg`, alt=`${o.title}`, data-object-fit="cover", data-object-position="center")
    +e.wrapper
      +e.container.container
        +e.title #{o.title}
        if o.subtitle
          +e.subtitle #{o.subtitle}